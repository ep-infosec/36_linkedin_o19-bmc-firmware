      "read_source" : {
        "util" : "/usr/local/bin/sensor-util"
      },
      "type": "linear",
      "positive_hysteresis": 0,
      "negative_hysteresis": 2,
      "data": [
        [ 0, 20 ],
        [ 76, 22 ],
        [ 78, 24 ],
        [ 80, 26 ],
        [ 82, 28 ],
        [ 84, 30 ],
        [ 86, 32 ],
        [ 88, 34 ],
        [ 90, 36 ],
        [ 91, 38 ],
        [ 92, 40 ],
        [ 93, 45 ],
        [ 94, 50 ],
        [ 95, 55 ]
