{
  "version": "fbttn-type5-mp-v3",
  "pwm_transition_value": 55,
  "pwm_boost_value": 50,
  "sample_interval_ms": 3000,
  "boost": {
    "fan_fail": false,
    "sensor_fail": true,
    "progressive": true
  },
  "fan_dead_boost": {
    "data": [
      [1,0],
      [2,100]
    ]
  },
  "sensor_valid_check": {
    "SOC": {
      "attribute": {
        "type" : "power_BIC_status",
        "fru" : "server",
        "number" : "55"
      }
    },
    "M2_SMART_Temp_1": {
      "attribute": {
        "type" : "gpio_power_nvme",
        "number" : "32",
        "fru" : "server",
        "nvme" : "1"
      }
    },
    "M2_SMART_Temp_2": {
      "attribute": {
        "type" : "gpio_power_nvme",
        "number" : "33",
        "fru" : "server",
        "nvme" : "2"
      }
    }
  },
  "fail_sensor_type": {
    "SSD_sensor_fail":false
  },
  "watchdog": false,
  "chassis_intrusion": false,
  "min_rpm": 400,
  "profiles": {
    "linear_soc_therm_margin": {
      "read_source" : {
        "util" : "/usr/local/bin/sensor-util"
      },
