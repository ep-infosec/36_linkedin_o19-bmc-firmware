{
  "shell_scripts" : {
    "board_utils.sh": {
      "read": [
        "/sys/class/i2c-adapter/i2c-12/12-0031/pwr_main_n",
        "/sys/class/i2c-adapter/i2c-12/12-0031/pwr_usrv_en",
        "/sys/class/i2c-adapter/i2c-12/12-0031/pwr_usrv_btn_en",
        "/sys/class/i2c-adapter/i2c-12/12-0031/slotid"

      ]
    },
    "cp2112_i2c_flush.sh": {
      "read": [
        "/sys/class/i2c-adapter/i2c-12/12-0031/i2c_flush_en"
      ]
    },
    "cpld_rev.sh": {
      "read": [
        "/sys/class/i2c-adapter/i2c-12/12-0031/cpld_rev",
        "/sys/class/i2c-adapter/i2c-12/12-0031/cpld_sub_rev"
      ]
    },
    "ec_version.sh": {
      "read": [
        "/sys/class/i2c-adapter/i2c-4/4-0033/version",
        "/sys/class/i2c-adapter/i2c-4/4-0033/date"
      ]
    },
    "reset_bcrm.sh": {
      "read": [
        "/sys/class/i2c-adapter/i2c-12/12-0031/th_sys_rst_n"
      ]
    },
    "reset_cp2112.sh": {
      "read": [
        "/sys/class/i2c-adapter/i2c-12/12-0031/usb2cp2112_rst_n"
      ]
    },
    "reset_qsfp_mux.sh": {
        "read": [
          "/sys/class/i2c-adapter/i2c-12/12-0031/i2c_mux0_rst_n",
          "/sys/class/i2c-adapter/i2c-12/12-0031/i2c_mux1_rst_n",
          "/sys/class/i2c-adapter/i2c-12/12-0031/i2c_mux2_rst_n",
          "/sys/class/i2c-adapter/i2c-12/12-0031/i2c_mux3_rst_n"
      ]
    },
    "setup_board.sh": {
      "read": [
        "/sys/class/i2c-adapter/i2c-12/12-0031/com-e_i2c_isobuf_en"
      ]
    },
    "wedge_power.sh": {
      "read": [
        "/sys/class/i2c-adapter/i2c-12/12-0031/pwr_cyc_all_n",
        "/sys/class/i2c-adapter/i2c-12/12-0031/usrv_rst_n",
        "/sys/class/i2c-adapter/i2c-12/12-0031/th_sys_rst_n",
        "/sys/class/i2c-adapter/i2c-12/12-0031/pwr_main_n"
      ]
    }
  }
}
